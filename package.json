{
  "name": "nuxt-unleash",
  "version": "1.0.5",
  "description": "Nuxt.js module to use Unleash toggle feature services",
  "keywords": [
    "nuxt",
    "nuxtjs",
    "unleash",
    "feature-flags",
    "module",
    "feature",
    "plugin"
  ],
  "homepage": "https://github.com/conejerock/nuxt-unleash",
  "bugs": {
    "url": "https://github.com/conejerock/nuxt-unleash/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/conejerock/nuxt-unleash.git"
  },
  "license": "MIT",
  "author": "Juanjo Conejero <juanjoconejero@gmail.com>",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "lint": "eslint --ext .js,.ts,.vue ./src",
    "release": "yarn test && yarn build && standard-version && git push --follow-tags && npm publish",
    "test": "yarn vitest --coverage"
  },
  "dependencies": {
    "@nuxt/kit": "^3.0.0-rc.8",
    "axios": "^0.27.2",
    "consola": "^2.15.3"
  },
  "devDependencies": {
    "@nuxt/module-builder": "latest",
    "@nuxt/test-utils": "latest",
    "@nuxtjs/eslint-config-typescript": "latest",
    "@vitest/coverage-c8": "^0.22.1",
    "eslint": "latest",
    "jest": "^29.0.1",
    "nuxt": "^3.0.0-rc.8",
    "prettier": "^2.7.1",
    "vitest": "^0.22.1"
  }
}
